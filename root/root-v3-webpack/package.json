{
  "name": "micro-pure-model",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "proto": "pbjs -t json-module -w es6 -o src/proto/proto.ts src/proto/*.proto && npm run proto-type",
    "proto-type": "pbjs -t static-module src/proto/*.proto | pbts -o src/proto/bundle.d.ts -",
    "serve": "webpack-dev-server -c ./webpack.dev.js --node-env=dev",
    "build": "webpack -c ./webpack.prod.js --node-env=production"
  },
  "dependencies": {
    "@micro-zoe/micro-app": "^1.0.0-rc.15",
    "axios": "^1.7.2",
    "pinia": "^2.1.7",
    "protobufjs": "^7.4.0",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/webpack-env": "^1.18.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "@webpack-cli/generators": "^3.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.0",
    "portfinder": "^1.0.32",
    "prettier": "^2.8.4",
    "style-loader": "^3.3.2",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.4.2",
    "typescript": "~4.8.4",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.25.2",
    "vue-loader": "^17.0.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.2.0",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0",
    "webpack-shell-plugin-next": "^2.3.1"
  }
}
